#include "nat.h"

using namespace std;

int main(int argc,char**argv)
{
	int c,pid;
	c=atio(argv[1]);
	pid = fork():
	if(pid ==0)
	{
		for(int x=1; x<=c; x++)
		{
			if(x%2 !=0)
			cout<<"Odd: "<<x<<" "<<endl;
		}
	}
	else
	{
		sleep(5);
		for(int y=1; y<=c; y++)
		{
			if(y%2 ==0)
				cout<<"Even :"<<y<<" "<<endl;
		}
	}
	return 0;
}
	
